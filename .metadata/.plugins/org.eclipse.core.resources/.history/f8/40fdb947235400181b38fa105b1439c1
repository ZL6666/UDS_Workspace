package com.teamcenter11.demo.command;

import com.teamcenter.rac.aif.AbstractAIFOperation;
import com.teamcenter.rac.kernel.TCComponent;
import com.teamcenter.rac.kernel.TCComponentFolderType;
import com.teamcenter.rac.kernel.TCComponentType;
import com.teamcenter.rac.kernel.TCSession;

public class CreateMyFolderOperation extends AbstractAIFOperation {
	
	private TCComponent tccomponent = null;
	private String folderName = null;
	private TCSession session = null;
	
	public CreateMyFolderOperation(TCSession session,TCComponent tccomponent,String folderName){
		this.tccomponent = tccomponent;
		this.folderName = folderName;
		this.session = session;
	}
	
	@Override
	public void executeOperation() throws Exception {
		TCComponentFolderType t = (TCComponentFolderType) session.getTypeComponent("Folder");
		t.create(folderName, "My Folder Description", "Folder");
		
	}

}
